{% if title %}
  <h3>{{ title }}</h3>
{% endif %}
<div class="dcf-grid-halves dcf-grid-thirds@sm dcf-grid-fourths@md dcf-grid-sixths@lg dcf-grid-sevenths@xl dcf-col-gap-vw dcf-row-gap-6 dcf-mt-6">
  {% for row in rows %}
    {%
      set row_classes = [
        default_row_class ? 'views-row',
      ]
    %}
    <div{{ row.attributes.addClass(row_classes) }}>
      {{- row.content -}}
    </div>
  {% endfor %}
</div>
